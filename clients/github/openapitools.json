{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.4.0",
    "generators": {
      "openapi": {
        "generatorName": "rust",
        "output": "./openapi",
        "glob": "./api/descriptions/ghes-3.4/ghes-3.4.yaml",
        "additionalProperties": {
          "packageVersion": "3.4.0",
          "library": "reqwest",
          "supportAsync": true
        },
        "ignoreFileOverride": ".openapi-generator-ignore"
      }
    }
  }
}
